	keywords = {Analytical models, animated 3D visualizations, animation, automotive components, camshafts, Computational modeling, Computer simulation, coordinated views, crankshaft, data visualisation, Data visualization, Drives, engine, engines, function graphs, graph theory, interactive visual analysis, mechanical engineering computing, Process design, segmented curve view, static 2D diagrams, Timing, timing chain drive, Valves},
	pages = {3--15},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\user\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\01q3cjjf.default\\zotero\\storage\\PZUPZP25\\4269942.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\user\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\01q3cjjf.default\\zotero\\storage\\WKCGJBRI\\Konyha et al. - 2007 - Interactive Visual Analysis of a Timing Chain Driv.pdf:application/pdf}
