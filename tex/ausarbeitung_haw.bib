	keywords = {available bandwidth measurement, Bandwidth, bandwidth probing techniques, bottleneck link capacity measurement, channel capacity, competing network traffic, Computerized monitoring, Computer science, gap model, initial gap increasing method, Internet, Internet measurements, IP networks, Length measurement, measurement accuracy, Measurement techniques, packet pair mechanism, packet switching, packet transmission rate method, Pathload, Performance Evaluation, probing packet gap, probing packet size, probing packet train length, queueing theory, queue size, simulations, single-hop network, Size measurement, telecommunication links, telecommunication traffic, Traffic control},
	pages = {879--894},
