	pages = {2301--2309},
	annote = {Vis Tool mit mehr freiheit als bei Highcharts},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Benny\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\6tnp53ur.default\\zotero\\storage\\GP2M5K3F\\articleDetails.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Benny\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\6tnp53ur.default\\zotero\\storage\\3Z5BX3W8\\Bostock et al. - 2011 - D #x0B3\; Data-Driven Documents.pdf:application/pdf}
