	pages = {31--40},
	file = {Snapshot:C\:\\Users\\user\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\01q3cjjf.default\\zotero\\storage\\N4NFNKQR\\abstract.html:text/html}
}

